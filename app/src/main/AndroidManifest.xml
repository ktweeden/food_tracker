<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.katepreston.food_tracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.AddFoodActivity"
            android:label="Add New Food"/>
        <activity android:name=".Activities.SingleFoodActivity" />
        <activity
            android:name=".Activities.EditFoodActivity"
            android:label="Edit Food"/>
        <activity android:name=".Activities.SingleMealActivity" />
        <activity
            android:name=".Activities.AddMealActivity"
            android:label="Add New Meal"/>
    </application>

</manifest>